{
  "name": "paint-socket",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "prebuild": "del-cli build",
    "start": "webpack serve --mode=development --hot",
    "build": "npm run prebuild && webpack --mode=production",
    "build:dev": "npm run prebuild && webpack --mode=development",
    "build:serve": "npm run prebuild && npm run build && npx serve -s build",
    "prettier": "npx prettier \"./**/*.{js,ts,jsx,tsx,scss,css}\" --write -u",
    "server": "npm run build && cd ./server && ts-node index.ts",
    "server:dev": "npm run build:dev && cd ./server && ts-node-dev index.ts --respawn --rs",
    "api": "cd ./server && ts-node-dev index.ts --respawn --rs",
    "fullstack": "concurrently --kill-others \"npm run start\" \"npm run api\""
  },
  "author": "vinsssten",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
